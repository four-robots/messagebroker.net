{
  "name": "KV_messages",
  "retention": "limits",
  "max_consumers": -1,
  "max_msgs_per_subject": 1,
  "max_msgs": -1,
  "max_bytes": -1,
  "max_age": 0,
  "max_msg_size": -1,
  "storage": "file",
  "discard": "new",
  "num_replicas": 1,
  "duplicate_window": 0,
  "placement": {
    "cluster": ""
  },
  "sources": [
    {
      "name": "KV_messages",
      "filter_subject": "$KV.messages.*.*",
      "external": {
        "api": "$JS.hub.API",
        "deliver": "deliver.hub.KV_messages.leaf-node-1"
      }
    },
    {
      "name": "KV_messages",
      "filter_subject": "$KV.messages.*.*.leaf-node-1",
      "external": {
        "api": "$JS.hub.API",
        "deliver": "deliver.hub.KV_messages.leaf-node-1"
      }
    }
  ],
  "subjects": [
    "$KV.messages.>"
  ],
  "sealed": false,
  "deny_delete": true,
  "deny_purge": false,
  "allow_rollup_hdrs": true,
  "allow_direct": true
}
